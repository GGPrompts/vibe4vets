  "va_tribal_programs": [
    {
      "name": "VHA Office of Tribal Health",
      "type": "va_program",
      "description": "Provides leadership, guidance, and strategic direction to support American Indian/Alaska Native Veteran health care and access. Oversees the IHS-VA MOU implementation.",
      "contact_email": "VHAOfficeofTribalHealth@va.gov",
      "website": "https://www.va.gov/health/vha-tribal-health.asp",
      "scope": "national"
    },
    {
      "name": "VA-IHS Clinic-in-a-Clinic Program",
      "type": "va_program",
      "description": "Places VA health care providers within IHS facilities to deliver VA services to Native Veterans. This 'one-stop shop' approach allows Native Veterans to access both VA and IHS services under one roof.",
      "locations": [
        {"name": "Chinle Service Unit", "state": "AZ", "opened": "2024"},
        {"name": "Kayenta Health Center", "state": "AZ", "opened": "2024"}
      ],
      "website": "https://www.ihs.gov/vaihsmou/",
      "scope": "local"
    },
    {
      "name": "VA Copayment Exemption Program for Native Veterans",
      "type": "va_program",
      "description": "Exempts enrolled American Indian and Alaska Native Veterans from VA health care copayments. Since April 2023, has exempted or reimbursed over 300,000 copayments totaling more than $5.3 million.",
      "eligibility": "Must be enrolled in VA health care and possess official tribal documentation (enrollment card, Certificate of Degree of Indian/Alaska Native Blood, or tribal letterhead documentation).",
      "how_to_apply": "Submit copies of tribal documents and completed VA Form 10-334 to: VHA Tribal Documentation, PO Box 5100, Janesville, WI 53547-5100",
      "website": "https://www.va.gov/health/vha-tribal-health.asp",
      "scope": "national"
    },
    {
      "name": "Native American Veteran Homelessness Initiative",
      "type": "federal_initiative",
      "description": "Multi-agency effort led by the White House Council on Native American Affairs Health Committee. Includes VA, HHS, and HUD working to address homelessness among Native American Veterans.",
      "website": "https://www.ihs.gov/vaihsmou/findhealthcaremap/",
      "scope": "national"
    }
  ],
