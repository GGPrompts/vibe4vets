# Database - use psycopg driver format
DATABASE_URL=postgresql+psycopg://vibe4vets:localdev@localhost:5432/vibe4vets

# API Keys
ANTHROPIC_API_KEY=

# VA.gov Lighthouse API (for VA facilities, Vet Centers, GI Bill schools)
# Get key at: https://developer.va.gov/
VA_API_KEY=

# CareerOneStop API (for American Job Centers, Apprenticeships)
# Get key at: https://www.careeronestop.org/Developers/WebAPI/registration.aspx
CAREERONESTOP_API_KEY=

# Environment
ENVIRONMENT=development
DEBUG=true

# CORS (JSON array format for pydantic-settings)
CORS_ORIGINS=["http://localhost:3000"]

# Admin API (hash of your admin API key)
ADMIN_API_KEY_HASH=
